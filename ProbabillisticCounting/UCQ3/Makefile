UCQ3_Main : UCQ3_Main.o 
	g++ -std=c++14 -O3 -o ProbabillisticCounting_UCQ3 UCQ3_Main.o -DINVERSE_MAPPING
	rm UCQ3_Main.o

UCQ3_Main.o : UCQ3_Main.cpp
	g++ -std=c++14 -O3 -c UCQ3_Main.cpp -DINVERSE_MAPPING
	

UCQ3_Main_Debug : UCQ3_Main_Debug.o 
	g++ -std=c++14 -g -o ProbabillisticCounting_UCQ3 UCQ3_Main.o -DINVERSE_MAPPING
	rm UCQ3_Main.o

UCQ3_Main_Debug.o : UCQ3_Main.cpp
	g++ -std=c++14 -g -c UCQ3_Main.cpp -DINVERSE_MAPPING
